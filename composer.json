{
    "name": "maaf/runtime",
    "description": "MAAF Runtime Package - Worker-based execution with cached bootstrap and module-level preload",
    "type": "library",
    "keywords": ["maaf", "runtime", "worker", "octane", "bootstrap", "preload"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bloch TamÃ¡s",
            "email": "info@bloch.hu"
        }
    ],
    "require": {
        "php": ">=8.1",
        "maaf/core": "^2.01"
    },
    "suggest": {
        "ext-opcache": "Required for opcache preload support"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "MAAF\\Runtime\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MAAF\\Runtime\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
